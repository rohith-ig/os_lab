[PROCESS_TABLE + ([SYSTEM_STATUS_TABLE+1]*16) + 4] = TERMINATED;
alias flag R0;
flag = 0;
alias i R1;
i = 1;
while (i<16) do
    if ([PROCESS_TABLE+(i*16)+4] == READY || [PROCESS_TABLE+(i*16)+4] == CREATED) then  
        flag = 1;
        break;
    endif;
    i = i + 1;
endwhile;
if (flag == 0) then
    print "Halt";
    halt;
else 
    call MOD_5;
endif;
halt;